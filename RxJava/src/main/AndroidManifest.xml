<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.hua.rxjava">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".OperatorsActivity">
        </activity>
        <activity android:name=".operator.CreatingActivity">
        </activity>
        <activity android:name=".operator.TransformingActivity">
        </activity>
        <activity android:name=".operator.FilteringActivity">
        </activity>

        <activity android:name=".operator.CombiningActivity">
        </activity>

    </application>

</manifest>